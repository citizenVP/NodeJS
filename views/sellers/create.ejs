<div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">

      </div>
      <div class="modal-body">
        <form action="/seller/create" method="post">
          <div class="form-group">
          <select class="form-control mb-2 mr-sm-2" data-style="btn-info" name="MarketName">
            <optgroup label="Market">
              <% markets.forEach(function(market){ %>
              <option class="form-control mb-2 mr-sm-2" name="MarketName" value="<%= market.Name %>">
                <%= market.Name %>
              </option>
              <% }) %>
            </optgroup>
          </select>
      </div>
      <div class="form-group">
        <input type="text" class="form-control mb-2 mr-sm-2" placeholder="FirstName" name="FirstName" autocomplete="off"
          required>
      </div>
      <div class="form-group">
        <input type="text" class="form-control mb-2 mr-sm-2" placeholder="LastName" name="LastName" autocomplete="off"
          required>
      </div>
      <div class="form-group">
        <input type="text" class="form-control mb-2 mr-sm-2" placeholder="Phone" name="Phone" autocomplete="off"
          required>
      </div>
      <div class="form-group">
        <input type="text" class="form-control mb-2 mr-sm-2" placeholder="Adress" name="Adress" autocomplete="off"
          required>
      </div>
      <div class="modal-footer center">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
        <button type="submit" class="btn btn-primary">Add</button>
      </div>
      </form>
    </div>
  </div>
</div>
</div>